    move_bounce_all(true);
    IncreaseBallSpeed();
    
    if State == "BlackBall"
    {
        instance_create(x, y, oExplosionBlackBall);
        State = "NormalBall";
        IsSpriteChanged = true;
    }
    else
    {
        with brick {
            if(CanBreak)
            {
                BrickHitDetected();
            }
        }
    }
}
