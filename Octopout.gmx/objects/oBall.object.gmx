            <string>/// collision with brick
var brick = instance_place(x + hspeed, y + vspeed, oDefaultBrick);

if instance_exists(brick)
{
    if(place_meeting(x + hspeed, y, oDefaultBrick))
    {
        while (!place_meeting(x + sign(hspeed), y, oDefaultBrick))
        {
            x += sign(hspeed);
        }
    }
    
    if(place_meeting(x, y + vspeed, oDefaultBrick))
    {
        while (!place_meeting(x, y + sign(vspeed), oDefaultBrick))
        {
            y += sign(vspeed);
        }
