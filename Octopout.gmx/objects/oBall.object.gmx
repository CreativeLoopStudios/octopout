            <kind>1</kind>
            <string>/// collision with player

if instance_exists(oPlayer) and place_meeting(x + hspeed, y + vspeed, oPlayer)
{

    if(place_meeting(x + hspeed, y, oPlayer))
    {
        while (!place_meeting(x + sign(hspeed), y, oPlayer))
        {
            x += sign(hspeed);
        }
    }
    
    if(place_meeting(x, y + vspeed, oPlayer))
    {
        while (!place_meeting(x, y + sign(vspeed), oPlayer))
        {
            y += sign(vspeed);
        }
    }
    
    
    if State != "BlackBall"
    {
        CountToCreateBlackBall += 1;
    }
    
    if(oPlayer.hspeed &lt; 0) {
        if(hspeed &gt;= 0) {
            move_bounce_all(true);
            IncreaseBallSpeed();
        } else if (hspeed &lt;= 0){
            direction = point_direction(oPlayer.x, oPlayer.y, x, y);
            DecreaseBallSpeed();
        } 
    } else if (oPlayer.hspeed &gt; 0){
        if(hspeed &lt;= 0) {
            move_bounce_all(true);
            IncreaseBallSpeed();
        } else  if (hspeed &gt;= 0){
            direction = point_direction(oPlayer.x, oPlayer.y, x, y);
            DecreaseBallSpeed();
        } 
    } else {
        move_bounce_all(true);
    }
}
